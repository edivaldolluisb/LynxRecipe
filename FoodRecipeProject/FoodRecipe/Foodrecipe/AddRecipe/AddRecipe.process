	}
	public void setrecipe_link(String val) {
		recipe_link = val;
	}
	public String getcalories() {
		return calories;
	}
	public void setcalories(String val) {
		calories = val;
	}
	public String getdish_type() {
		return dish_type;
	}
	public void setdish_type(String val) {
		dish_type = val;
	}
	public String getdiet_type() {
		return diet_type;
	}
	public void setdiet_type(String val) {
		diet_type = val;
	}
	public String getXMLresult() {
		return XMLresult;
	}
