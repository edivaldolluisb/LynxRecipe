		XMLresult = val;
	}
/****** END SET/GET METHOD, DO NOT MODIFY *****/
	public AddRecipeJSONtoXML() {
	}
	public void invoke() throws Exception {
/* Available Variables: DO NOT MODIFY
	In  : String name
	In  : String ingredients
	In  : String preparation
	In  : String image_url
	In  : String recipe_link
	In  : String calories
	In  : String dish_type
	In  : String diet_type
	Out : String XMLresult
* Available Variables: DO NOT MODIFY *****/

XMLresult = "&lt;recipe&gt;&lt;name&gt;"+name+"&lt;/name&gt;&lt;ingredients&gt;"+ingredients+"&lt;/ingredients&gt;&lt;preparation&gt;"+preparation+"&lt;/preparation&gt;&lt;image_url&gt;"+image_url+"&lt;/image_url&gt;&lt;recipe_link&gt;"+recipe_link+"&lt;/recipe_link&gt;&lt;calories&gt;"+calories+"&lt;/calories&gt;&lt;dish_type&gt;"+dish_type+"&lt;/dish_type&gt;&lt;diet_type&gt;"+diet_type+"&lt;/diet_type&gt;&lt;/recipe&gt;";}
}
</fullsource>
