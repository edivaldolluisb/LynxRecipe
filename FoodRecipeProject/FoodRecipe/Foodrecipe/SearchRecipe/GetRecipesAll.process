                    </ns1:recipes>
                </xsl:when>
                <xsl:when test="$Start/root/filter  =&quot;all&quot;">
                    <ns1:recipes>
                        <xsl:for-each select="$Get-Recipes/resultSet/Record">
                            <ns1:recipe>
                                <ns1:recipe_name>
                                    <xsl:value-of select="recipe_name"/>
                                </ns1:recipe_name>
                                <xsl:if test="recipe_number">
                                    <ns1:recipe_number>
                                        <xsl:value-of select="recipe_number"/>
                                    </ns1:recipe_number>
                                </xsl:if>
                                <xsl:if test="ingredient">
                                    <ns1:ingredient>
                                        <xsl:value-of select="ingredient"/>
                                    </ns1:ingredient>
                                </xsl:if>
                                <xsl:if test="preparation">
                                    <ns1:preparation>
                                        <xsl:value-of select="preparation"/>
                                    </ns1:preparation>
                                </xsl:if>
                                <ns1:creation_date>
                                    <xsl:value-of select="creation_date"/>
                                </ns1:creation_date>
                                <xsl:if test="image_url">
                                    <ns1:image_url>
                                        <xsl:value-of select="image_url"/>
                                    </ns1:image_url>
                                </xsl:if>
                                <xsl:if test="recipe_link">
                                    <ns1:recipe_link>
                                        <xsl:value-of select="recipe_link"/>
                                    </ns1:recipe_link>
                                </xsl:if>
                                <xsl:if test="calories">
                                    <ns1:calories>
                                        <xsl:value-of select="calories"/>
                                    </ns1:calories>
                                </xsl:if>
                                <ns1:dish_type>
                                    <xsl:value-of select="dish_type"/>
                                </ns1:dish_type>
                                <ns1:diet_type>
                                    <xsl:value-of select="diet_type"/>
                                </ns1:diet_type>
                            </ns1:recipe>
                        </xsl:for-each>
                    </ns1:recipes>
                </xsl:when>
                <xsl:when test="$Start/root/filter  =&quot;name&quot;">
                    <ns1:recipes>
                        <xsl:copy-of select="$name/ns1:recipes/ns1:recipe"/>
                    </ns1:recipes>
                </xsl:when>
                <xsl:when test="$Start/root/filter  =&quot;preparation&quot;">
                    <ns1:recipes>
                        <xsl:copy-of select="$preparation/ns1:recipes/ns1:recipe"/>
                    </ns1:recipes>
                </xsl:when>
                <xsl:when test="$Start/root/filter  =&quot;ingredient&quot;">
                    <ns1:recipes>
                        <xsl:copy-of select="$ingredient/ns1:recipes/ns1:recipe"/>
                    </ns1:recipes>
                </xsl:when>
            </xsl:choose>
            <xsl:choose>
                <xsl:when test="exists($_error/ns:ErrorReport/MsgCode )">
                    <error>
                        <code>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </code>
                        <message>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </message>
                        <microServiceType>
                            <xsl:value-of select="'Get All Recipes'"/>
                        </microServiceType>
                        <sucess>
                            <xsl:value-of select="0"/>
                        </sucess>
                    </error>
                </xsl:when>
                <xsl:otherwise>
                    <error>
                        <code>
                            <xsl:value-of select="200"/>
                        </code>
                        <message>
                            <xsl:value-of select="'Returned with sucess'"/>
                        </message>
                        <microServiceType>
                            <xsl:value-of select="'Get All Recipes'"/>
                        </microServiceType>
                        <sucess>
                            <xsl:value-of select="1"/>
                        </sucess>
                    </error>
                </xsl:otherwise>
            </xsl:choose>
